<?xml version="1.0" encoding="UTF-8"?>
<web-app>
     <display-name>Search Engine Master</display-name>
     
     <servlet>
         <servlet-name>SearchEngineServlet</servlet-name>
         <servlet-class>edu.upenn.cis455.servlets.SearchEngineServlet</servlet-class>
         <load-on-startup/>
         <init-param>
           <param-name>checkerdbdir</param-name>
           <param-value>/home/ec2-user/checker_db/</param-value>
         </init-param>
     </servlet>
     
     <servlet-mapping>
         <servlet-name>SearchEngineServlet</servlet-name>
         <url-pattern>/*</url-pattern>
     </servlet-mapping>
</web-app>
